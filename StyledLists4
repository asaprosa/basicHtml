        .item2 { text-decoration: overline; color: #2196f3; }
        .item3 { text-decoration: line-through; color: #4caf50; }
        .item4 { text-decoration: underline overline; color: #9c27b0; }
        .item5 { text-decoration: wavy underline; color: #ff9800; }
    </style>
